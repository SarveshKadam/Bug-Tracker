<h2>Defects</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Number</th>
              <th>Assigned To</th>
              <th>State</th>
              <th>Created At</th>
              <th>Environment</th>
            </tr>
          </thead>
          <tbody>
            <% defects.forEach( defect =>{ %> 
              <tr>
                <td><a href="/defects/<%= defect.id %>"><%= defect.number %></a></td>
                <td><%= defect.member.name %></td>
                <td><%= defect.state %></td>
                <td><%= defect.createdOn.toISOString().split('T')[0] %></td>
                <td><%= defect.environment %></td>
              </tr>
              <% }) %> 
          </tbody>
        </table>
      </div>