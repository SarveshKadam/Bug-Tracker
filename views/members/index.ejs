<h2>Section title</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Number</th>
              <th>FirstName</th>
              <th>LastName</th>
              <th>Email</th>
              <th>Position</th>
            </tr>
          </thead>
          <tbody>
            <% members.forEach( member =>{ %> 
            <tr>
              <td><a href="/members/<%= member.id %>"><%= member.memberID %></a></td>
              <td><%= member.firstName %></td>
              <td><%= member.lastName %></td>
              <td><%= member.email %></td>
              <td><%= member.position %></td>
            </tr>
            <% }) %> 
          </tbody>
        </table>
      </div>